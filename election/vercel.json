{

  "builds": [{
    "src": "election/wsgi.py",
    "use": "@vercel/python",
    "config": {"maxLambdaSize": "15mb", "runtime":  "python3.9"}
  },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {"DistDir": "staticfile_build"}
    }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "election/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    }
  ]
}